								"exec": [
									"if (pm.response.code === 200 && pm.response.json().success) {",
									"    const responseData = pm.response.json().data;",
									"    if (responseData.token) {",
									"        pm.collectionVariables.set('auth_token', responseData.token);",
									"        console.log('Auth token saved');",
									"    }",
