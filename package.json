{
  "name": "ywc-api",
  "version": "1.0.0",
  "description": "YWC Financial Forms API - A robust Node.js TypeScript API built with Express.js and PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "ts-node src/seed/seeder.ts",
    "seed:reset": "ts-node src/seed/seeder.ts --reset",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:postman": "node src/postman/run-tests.js",
    "test:api": "npm run test:postman"
  },
  "keywords": [
    "api",
    "typescript",
    "express",
    "postgresql",
    "financial",
    "forms"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.24",
    "@types/pg": "^8.12.0",
    "newman": "^6.1.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
